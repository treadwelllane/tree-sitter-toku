{
  "name": "toku",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "content"
        },
        {
          "type": "SYMBOL",
          "name": "template"
        }
      ]
    },
    "content": {
      "type": "STRING",
      "value": "content"
    },
    "template": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<%"
        },
        {
          "type": "SYMBOL",
          "name": "template_content"
        },
        {
          "type": "STRING",
          "value": "%>"
        }
      ]
    },
    "template_content": {
      "type": "PATTERN",
      "value": "([^%]|%[^>])*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
